{
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": ["plugin:react/recommended", "airbnb"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module",
      "allowImportExportEverywhere": true,
      "ecmaVersion": 2018
    },
    "plugins": ["react"],
    "rules": {
      "semi": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "quotes": 0
    }
  }
  